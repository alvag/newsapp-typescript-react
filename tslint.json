{
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "no-duplicate-variable": true,
        "no-unused-variable": true,
        "semicolon": [
            true,
            "always"
        ],
        "no-console": true,
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "max-line-length": false,
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "jsx-no-multiline-js": false
    }
}
